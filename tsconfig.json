{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM"],
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "noImplicitReturns": true,
    "strict": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}